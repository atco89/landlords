<!doctype html>
<html lang="sr-RS">
    <head>
        <base href="{{ app.request.schemeAndHttpHost ~ app.request.baseUrl }}"/>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1, minimum-scale=1, maximum-scale=1">
        <title>Landlords :: {% block title %}{% endblock %}</title>
        <!--suppress HtmlUnknownTarget -->
        <link type="image/png" rel="icon" href="./media/icons/favicon.ico">
        <!--suppress JSUnresolvedLibraryURL -->
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css" rel="stylesheet"
              integrity="sha384-rbsA2VBKQhggwzxH7pPCaAqO46MgnOM80zW1RWuH61DGLwZJEdK2Kadq2F9CUG65"
              crossorigin="anonymous">
        <!--suppress JSUnresolvedLibraryURL -->
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.1.1/css/all.min.css"
              integrity="sha512-KfkfwYDsLkIlwQp6LFnl8zNdLGxu9YAA1QvwINks4PhcElQSvqcyVLLD9aMhXd13uQjoXtEKNosOWaZqXgel0g=="
              crossorigin="anonymous" referrerpolicy="no-referrer"/>
        <!--suppress HtmlUnknownTarget -->
        <link rel="stylesheet" href="./css/style.css">
        {% block stylesheets %}{% endblock %}
    </head>
    <body>
        <div class="container-fluid">
            {% block preloader %}
                {% include './common/loader/index.html.twig' %}
            {% endblock %}

            {% block header %}
                {% include './common/header/index.html.twig' %}
            {% endblock %}

            <div id="main" class="container">
                {% block container %}{% endblock %}
            </div>

            {% block footer %}
                {% include './common/footer/index.html.twig' %}
            {% endblock %}
        </div>
        <!--suppress JSUnresolvedLibraryURL -->
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.js"></script>
        <!--suppress HtmlUnknownTarget -->
        <script src="./js/app.js"></script>
        {% block scripts %}{% endblock %}
    </body>
</html>
